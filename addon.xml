<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.audio.music" name="music" version="1.0.0" provider-name="sean">
  <requires>
    <import addon="xbmc.python" version="3.0.1"/>
    <import addon="script.module.xbmcswift2" version="2.4.0"/>
    <import addon="script.module.beautifulsoup4" version="4.5.3"/>
    <import addon="script.module.requests" version="2.19.1"/>
    <import addon="script.module.html5lib" version="0.999.0"/>
    <import addon="script.module.future" version="0.16.0"/>
    <import addon="script.module.qrcode" version="5.3"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.python.module" library="api.py"/>
  <extension point="xbmc.python.module" library="encrypt.py"/>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <language></language>
    <assets>
      <icon>icon.png</icon>
    </assets>
    <description>音乐是一款专注于发现与分享的音乐产品</description>
  </extension>
